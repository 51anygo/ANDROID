android:layout_width="wrap_content"
android:layout_height="-9px" android:layout_marginLeft="10px" android:text="@string/setting_phonenumtip">
</TextView><EditText android:layout_height="wrap_content" android:id="@+id/EditTextPhoneNum" android:singleLine="true" android:layout_width="fill_parent" android:layout_marginLeft="10px" android:password="false" android:phoneNumber="true"></EditText>
<Button android:layout_width="wrap_content" 
android:layout_height="wrap_content" 
android:text="@string/setting_logintesttip" 
android:id="@+id/ButtonUsrReg" android:layout_marginLeft="10px"></Button>
<TextView
android:layout_width="wrap_content"
